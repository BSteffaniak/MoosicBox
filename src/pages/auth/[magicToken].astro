---
import Layout from '~/layouts/Layout.astro';
import AuthPage from '~/routes/auth/[magicToken].tsx';

const search = Object.fromEntries(new URLSearchParams(Astro.url.searchParams));
const { magicToken } = Astro.params;
---

<Layout title="MoosicBox">
    <AuthPage client:only magicToken={magicToken ?? ''} search={search} />
</Layout>
